{
  "expo": {
    "name": "Wall-E",
    "slug": "Wall-E",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.akjohn2304.WallE"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Image browsing app",
    "githubUrl": "https://github.com/amaljohn2304/Wall-E/tree/master"
  }
}
